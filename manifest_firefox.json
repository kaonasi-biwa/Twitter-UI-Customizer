{
    "name": "__MSG_extensionName__",
    "description": "__MSG_extensionDescription__",
    "version": "2.10.1",
    "default_locale": "ja",
    "manifest_version": 2,
    "icons": {
        "16": "icon/icon16.png",
        "48": "icon/icon48.png",
        "128": "icon/icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.twitter.com/*"
            ],
            "js": [
                "scr/observer.js",
                "scr/data.js",
                "scr/library.js",
                "scr/option.js",
                "scr/applyCSS.js",
                "scr/index.js"
            ],
            "run_at": "document_start"
        },
          {
            "matches": [
                "*://*.twitter.com/*"
            ],
            "js": [
                "scr/runningSafemode.js"
            ],
            "run_at": "document_end"
        }
    ],
    "options_ui": {
        "page": "TUIC/options/options.html",
        "browser_style": true
    },
    "permissions": [
        "notifications",
        "storage",
        "https://github.com/kaonasi-biwa/Twitter-UI-Customizer/releases/latest/download/*",
        "https://objects.githubusercontent.com/*",
        "https://*.mico.re/*",
        "https://abs.twimg.com/*"
    ],
    "browser_action": {
        "default_icon": {
            "16": "icon/icon16.png",
            "48": "icon/icon48.png",
            "128": "icon/icon128.png"
        },
        "default_title": "Twitter UI Customizer",
        "default_popup": "TUIC/popup/popup.html"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "applications": {
        "gecko": {
            "id": "{e9c237f9-8c89-4f60-aa0d-e17e305398dc}"
        }
    },
    "developer": {
        "name": "kaonasi_biwa",
        "url": "https://github.com/kaonasi-biwa/Twitter-UI-Customizer"
    }
}
