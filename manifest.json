{
    "applications": {
        "gecko": {
          "id": "{e9c237f9-8c89-4f60-aa0d-e17e305398dc}"
        }
      },
    "name": "Twitter UI Customizer",
    "version": "2.2.1",
    "manifest_version": 2,
    "description": "TwitterのUIのカスタマイズ性を向上させます！",
    "icons": {
        "16": "icon/icon16.png",
        "48": "icon/icon48.png",
        "128": "icon/icon128.png"
    },
    "permissions": [
        "tabs",
        "declarativeContent",
        "storage"
    ],
    "content_scripts": [
          {
            "matches": [
                "*://*.twitter.com/*"
            ],
            "js": [
                "scr/jquery-3.6.0.min.js",
                "scr/style.js"
            ]
        }
    ],
    "background": {
        "scripts": [
            "scr/background.js"
        ],
        "persistent": false
    },

    "browser_action": {
        "default_icon": {
            "16": "icon/icon16.png",
            "48": "icon/icon48.png",
            "128": "icon/icon128.png"
        },
        "default_title": "Twitter UI Customizer",
        "default_popup": "scr/popup.html"
    },
    "options_page": "scr/options.html",
    "options_ui": {
        "page":"scr/options.html"
    },
    "developer": {
      "name": "kaonasi_biwa",
      "url": "https://github.com/kaonasi-biwa/Twitter-UI-Customizer"
    }
}