{
    "name": "Twitter UI Customizer",
    "version": "2.4.6",
    "manifest_version": 2,
    "description": "TwitterのUIのカスタマイズ性を向上させます！",
    "icons": {
        "16": "icon/icon16.png",
        "48": "icon/icon48.png",
        "128": "icon/icon128.png"
    },
    "content_scripts": [
          {
            "matches": [
                "*://*.twitter.com/*"
            ],
            "js": [
                "scr/style.js"
            ]
        }
    ],
    "options_ui": {
        "page":"scr/options.html"
    },
    "permissions": [
        "tabs",
        "notifications",
        "https://raw.githubusercontent.com/kaonasi-biwa/Twitter-UI-Customizer/main/*"
    ],
    "browser_action": {
        "default_icon": {
            "16": "icon/icon16.png",
            "48": "icon/icon48.png",
            "128": "icon/icon128.png"
        },
        "default_title": "Twitter UI Customizer",
        "default_popup": "scr/popup.html"
    },
    "background":{
        "scripts":["background.js"]
    },
    "applications": {
        "gecko": {
          "id": "{e9c237f9-8c89-4f60-aa0d-e17e305398dc}"
        }
      },
    "developer": {
      "name": "kaonasi_biwa",
      "url": "https://github.com/kaonasi-biwa/Twitter-UI-Customizer"
    }
}